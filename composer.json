{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "darryldecode/cart": "^4.2",
        "google/apiclient": "^2.18",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "joedixon/laravel-translation": "^2.2",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.8",
        "mckenziearts/laravel-notify": "^2.5",
        "php-flasher/flasher-laravel": "^1.15",
        "php-flasher/flasher-sweetalert-laravel": "^1.15",
        "php-flasher/flasher-toastr-laravel": "^1.15",
        "proengsoft/laravel-jsvalidation": "^4.8",
        "pusher/pusher-php-server": "*",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-translatable": "^6.5",
        "torann/currency": "^1.1",
        "tymon/jwt-auth": "^2.1",
        "yajra/laravel-datatables": "^10.0",
        "yoeunes/toastr": "^2.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/main_helper.php",
            "app/Helpers/notification_helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
